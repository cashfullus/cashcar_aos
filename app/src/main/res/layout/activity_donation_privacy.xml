<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.donation.DonationPrivacyActivity">

    <com.cashfulus.cashcarplus.view.UpgradedToolbar
        android:id="@+id/toolbarDonationPolicy"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:leftButtonImg="@drawable/ic_btn_back"
        app:layout_collapseMode="pin"
        app:title="개인정보 제3자 제공"
        app:layout_constraintTop_toTopOf="parent"/>

    <WebView
        android:id="@+id/wvDonationPolicy"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@+id/toolbarDonationPolicy"
        app:layout_constraintBottom_toBottomOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>