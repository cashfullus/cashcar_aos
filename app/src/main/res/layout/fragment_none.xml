<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="com.cashfulus.cashcarplus.ui.NoneFragment">

    <ImageView
        android:id="@+id/ivTabNone"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:src="@drawable/tab_prepare"
        app:layout_constraintDimensionRatio="h, 171:233"
        android:layout_marginHorizontal="86dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/tvTabNone"
        app:layout_constraintVertical_chainStyle="packed"/>

    <TextView
        android:id="@+id/tvTabNone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="서비스를 준비 중입니다\n조금만 기다려주세요 :)"
        style="@style/body_16_R_lh26"
        android:textColor="@color/grayscale_500"
        app:layout_constraintTop_toBottomOf="@+id/ivTabNone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="36dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>